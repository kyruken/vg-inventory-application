<div class="title-container">
    <h2>Developer's List</h2>
    <a href="/games/developer_form/create"><button>+ Add new developer</button></a>
</div>

<div class="content-container dark-box">
    <% for (let x = 0; x < developers.length; x++) { %>
        <h3><a href='<%= developers[x].url %>'><button class="red-button button-in-flex"><%= developers[x].name %> </button></a></h3>
    
    <% } %>
</div>
