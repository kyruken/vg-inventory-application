<h2><%= name %></h2>
<form method="POST" action=''>
    <label>
        <input type="text" name="title" placeholder="title">
    </label>
    <label>
        <select name="developer" id="developer">
            <% for (let x = 0; x < result.developers.length; x++) { %>
                <option><%= result.developers[x].name %></option>


            <% } %>
        </select>
    </label>
    <label>
        ESRB:
        <select name="esrb" id="esrb">
            <option>E</option>
            <option>E10</option>
            <option>T</option>
            <option>M</option>
        </select>
    </label>
    <label>
        <% for (let x = 0; x < result.genres.length; x++) { %>
            <%= result.genres[x].name %>
            <input type="checkbox" name="genre" placeholder="genre" value="<%=result.genres[x].name%>"">
        <% } %>
    </label>
    <label>
        <input type="date" name="release" placeholder="release">
    </label>
    <label>
        <input type="number" name="price" placeholder="price">
    </label>
    <button type="submit">Submit</button>
</form>
